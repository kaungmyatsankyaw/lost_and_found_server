{
    "name": "appstract/laravel-response-macros",
    "description": "Extra Response Macro's for Laravel",
    "keywords": ["appstract", "laravel", "response", "macros", "php"],
    "license": "MIT",
    "authors": [
        {
            "name": "Bob Krijnen",
            "email": "hello@appstract.team",
            "homepage": "https://appstract.team",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6",
        "laravel/framework": ">=5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Appstract\\ResponseMacros\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Appstract\\ResponseMacros\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Appstract\\ResponseMacros\\ResponseMacrosServiceProvider"
            ]
        }
    }
}
